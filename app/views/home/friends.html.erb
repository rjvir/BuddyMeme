Your top friends<hr><br />

<% count = 0 %>
<% array = [] %>
<% @top_friends["query2"].each do |top| %>
		<% array.push(top) %>
<% end %>

<% random_array = array.sort_by{rand} %>

<% random_array.each do |person| %>
	<% if count < 5 %>
		<%= link_to person["name"], photos_path(:id => person["uid"]) %> 
		<%= link_to image_tag(person["pic_square"]), photos_path(:id => person["uid"]) %>
		<% count+=1 %>
	<% end %>
<% end %>

